{
    "name": "aaemnnosttv/dev-debug",
    "type": "wordpress-muplugin",
    "description": "Development debugging functions for WordPress",
    "keywords": ["debug", "wordpress"],
    "authors": [
    	{
    		"name": "Evan Mattson",
    		"email": "me@aaemnnost.tv",
    		"homepage": "https://aaemnnost.tv",
    		"role": "Developer"
    	}
    ],
    "repositories": [
        {"type": "composer", "url": "https://wpackagist.org"}
    ],
    "autoload": {
        "classmap": ["classes"],
        "files": ["src/functions.php"],
        "psr-4": {
            "DevDebug\\": "src"
        }
    },
    "require": {
	    "composer/installers": "^1.0"
    },
    "require-dev": {
        "roots/wordpress": "^5",
        "wpackagist-plugin/debug-bar": "^1"
    }
}
